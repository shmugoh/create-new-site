{
    "name": "jago",
    "description": "Static Markdown Site Generator with a strong focus on client performance and flexibility",
    "author": "shmugo <juan@shmugo.co> (https://shmugo.co/)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/juanpisss/create-new-site.git"
    },
    "license": "MIT",
    "version": "0.4.0",
    "keywords": [
        "static",
        "generator",
        "website",
        "markdown",
        "site",
        "ejs"
    ],
    "engine": {
        "node": ">=18.0.0"
    },
    "files": [
        ".\\dist\\*"
    ],
    "bin": ".\\dist\\main.js",
    "scripts": {
        "start": "ts-node --esm main.ts",
        "build:node": "copyfiles -a template\\**\\* dist\\ && tsc --project tsconfig-node.json"
    },
    "dependencies": {
        "@exampledev/new.css": "^1.1.3",
        "chalk": "^5.3.0",
        "clean-css": "^5.3.2",
        "commander": "^11.1.0",
        "copyfiles": "^2.4.1",
        "ejs": "^3.1.9",
        "html-minifier": "^4.0.0",
        "showdown": "^2.1.0",
        "yaml": "^2.3.3"
    },
    "devDependencies": {
        "@tsconfig/node18": "^18.2.2",
        "@types/clean-css": "^4.2.9",
        "@types/ejs": "^3.1.4",
        "@types/html-minifier": "^4.0.4",
        "@types/node": "^20.8.7",
        "@types/showdown": "^2.0.3",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "prettier": "^3.0.3",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2"
    }
}
